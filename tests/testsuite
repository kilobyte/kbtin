    ) 5>&1 2>&1 | eval $at_tee_pipe
    at_status=`cat "$at_status_file"`
    ;;

  4 ) # 4. tests.at:25: #showme with non-ASCII chars
    at_setup_line='tests.at:25'
    at_desc='#showme with non-ASCII chars'
    $at_quiet $ECHO_N "  4: #showme with non-ASCII chars                 $ECHO_C"
    at_xfail=no
    echo "#                             -*- compilation -*-" >> "$at_group_log"
    (
      echo "4. tests.at:25: testing ..."
      $at_traceon

