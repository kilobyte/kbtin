    at_status=`cat "$at_status_file"`
    ;;

  3 ) # 3. tests.at:11: #showme
    at_setup_line='tests.at:11'
    at_desc='#showme'
    $at_quiet $ECHO_N "  3: #showme                                      $ECHO_C"
    at_xfail=no
    echo "#                             -*- compilation -*-" >> "$at_group_log"
    (
      echo "3. tests.at:11: testing ..."
      $at_traceon

