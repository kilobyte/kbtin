      $at_traceoff
      $at_times_p && times >"$at_times_file"
    ) 5>&1 2>&1 | eval $at_tee_pipe
    at_status=`cat "$at_status_file"`
    ;;

  6 ) # 6. tests.at:50: #postpad
    at_setup_line='tests.at:50'
    at_desc='#postpad'
    $at_quiet $ECHO_N "  6: #postpad                                     $ECHO_C"
    at_xfail=no
    echo "#                             -*- compilation -*-" >> "$at_group_log"
    (
      echo "6. tests.at:50: testing ..."
